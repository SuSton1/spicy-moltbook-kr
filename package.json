{
  "name": "nextapp_tmp",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint .",
    "typecheck": "tsc --noEmit",
    "format": "prettier -w .",
    "test": "VITE_CJS_IGNORE_WARNING=1 vitest run",
    "verify": "npm run lint && npm run typecheck && npm run test",
    "postverify": "bash tools/prod-gate.sh",
    "postinstall": "node tools/install-githooks.mjs || true",
    "hooks:install": "node tools/install-githooks.mjs",
    "db:up": "docker compose up -d",
    "db:down": "docker compose down",
    "db:logs": "docker compose logs -f --tail=200",
    "db:migrate": "prisma migrate dev",
    "db:seed": "node prisma/seed.mjs",
    "db:studio": "prisma studio",
    "seed:portal": "node scripts/seed-portal.mjs",
    "ops:remote:patch-hook": "bash tools/ops/remote-patch-post-receive.sh",
    "ops:probe:live": "bash tools/ops/probe-live-connectivity.sh",
    "ops:release-gate": "bash tools/release-gate.sh",
    "prod:check": "bash tools/prod-check.sh",
    "prod:recover": "bash tools/prod-recover.sh",
    "prod:diagnose": "bash tools/prod-diagnose.sh",
    "prod:diagnose:signup": "bash tools/prod-diagnose-signup.sh",
    "prod:gate": "bash tools/prod-gate.sh",
    "prod:origin:init": "bash tools/prod-init-origin-id.sh",
    "prod:origin:show": "bash tools/prod-origin-id.sh",
    "prod:smoke:signup": "bash tools/prod-signup-smoke.sh",
    "finalize": "SKIP_PROD_GATE=1 npm run verify && SKIP_PROD_GATE=1 npm run build && npm run prod:origin:init && npm run prod:gate",
    "deploy:prod": "bash tools/deploy-prod.sh",
    "deploy:prod:dry": "bash tools/deploy-prod.sh --dry-run",
    "setup:autodeploy": "tools/autodeploy/setup_once.sh",
    "autodeploy": "tools/autodeploy/auto_commit_push.sh",
    "skill:check": "node scripts/skill-check.mjs",
    "agent:public:check": "node scripts/agent-public-check.mjs",
    "nick:backfill-human": "node scripts/backfill-human-nicknames.mjs"
  },
  "dependencies": {
    "@auth/prisma-adapter": "2.11.0",
    "@prisma/adapter-pg": "^7.3.0",
    "@prisma/client": "^7.3.0",
    "bcryptjs": "^3.0.3",
    "next": "16.1.6",
    "next-auth": "5.0.0-beta.30",
    "pg": "^8.18.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "spicy-moltbook-agent": "file:./runner"
  },
  "devDependencies": {
    "@playwright/test": "^1.58.1",
    "@types/node": "20.19.30",
    "@types/nodemailer": "^7.0.9",
    "@types/pg": "^8.10.10",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "babel-plugin-react-compiler": "1.0.0",
    "dotenv": "^17.2.3",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "prettier": "^3.2.5",
    "prisma": "^7.3.0",
    "typescript": "^5",
    "vitest": "^2.1.1"
  }
}
