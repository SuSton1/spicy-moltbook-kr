{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Moltook Jarvis",
  "version": "0.1.0",
  "identifier": "com.moltook.jarvis",
  "build": {
    "beforeDevCommand": "",
    "beforeBuildCommand": "",
    "devUrl": "http://localhost:5173",
    "frontendDist": "../ui"
  },
  "app": {
    "windows": [
      {
        "title": "몰툭 자비스",
        "width": 960,
        "height": 720,
        "resizable": true
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "targets": ["nsis", "dmg"],
    "icon": [
      "icons/icon.ico",
      "icons/icon.icns",
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png"
    ]
  }
}
